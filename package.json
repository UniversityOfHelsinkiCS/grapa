{
  "name": "grapa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "docker compose up",
    "start:prod": "NODE_ENV=production node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/server/index.ts",
    "start:test": "NODE_ENV=test node --loader ts-node/esm src/server/index.ts",
    "start:dev": "NODE_ENV=development concurrently \"nodemon --exec node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/server/index.ts\" \"vite\"",
    "start:dev:front": "vite",
    "start:dev:server": "NODE_ENV=development ts-node-dev src/server/index.ts",
    "test:integration": "concurrently \"docker run -e PGDATA=/test-data -e POSTGRES_PASSWORD=postgres -p 5433:5432 postgres:15.6\" \"NODE_ENV=test DATABASE_URL=postgres://postgres:postgres@db:5433/postgres jest\" --hide 0 --prefix none",
    "test:integration:watch": "concurrently \"docker run -e PGDATA=/test-data -e POSTGRES_PASSWORD=postgres -p 5433:5432 postgres:15.6\" \"NODE_ENV=test DATABASE_URL=postgres://postgres:postgres@db:5433/postgres jest --watch\" --hide 0 --prefix none",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write '*.{ts,tsx,json,css,md}'",
    "build": "DISABLE_ESLINT_PLUGIN=true vite build",
    "prepare": "husky install"
  },
  "jest": {
    "testMatch": [
      "**/*.integration-test.ts"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.12",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@mui/x-data-grid": "^7.3.1",
    "@mui/x-date-pickers": "^7.3.1",
    "@sentry/browser": "^7.110.1",
    "@sentry/node": "^7.110.1",
    "@tanstack/react-query": "^5.32.0",
    "@tanstack/react-query-devtools": "^5.32.0",
    "axios": "^1.6.8",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.0",
    "i18next": "^23.11.2",
    "ioredis": "^5.3.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "notistack": "^3.0.1",
    "npm": "^10.6.0",
    "openid-client": "^5.6.5",
    "passport": "^0.7.0",
    "pg": "^8.11.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3",
    "sequelize": "^6.37.3",
    "umzug": "^3.8.0",
    "winston": "^3.13.0",
    "winston-gelf-transporter": "^1.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/register": "^7.23.7",
    "@types/connect-redis": "^0.0.23",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^8.56.9",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/passport": "^1.0.16",
    "@types/prettier": "^3.0.0",
    "@types/react-dom": "^18.2.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.9"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb-typescript",
      "prettier",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "react",
      "@typescript-eslint",
      "react-hooks",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "project": "./tsconfig.json"
    },
    "rules": {
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function",
          "unnamedComponents": "arrow-function"
        }
      ],
      "react/jsx-props-no-spreading": "off",
      "import/no-cycle": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.integration-test.*",
          "**/*.test.*"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint",
    "*.{ts,tsx,json,css,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000"
}
